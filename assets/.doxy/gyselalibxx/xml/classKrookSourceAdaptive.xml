      <memberdef kind="function" id="classKrookSourceAdaptive_1a4a7cd940383e05f3425a9023324ee037" prot="public" static="no" const="yes" explicit="no" inline="no" virt="non-virtual">
        <type>void</type>
        <definition>void KrookSourceAdaptive::get_derivative</definition>
        <argsstring>(DFieldSpXVx df, DConstFieldSpXVx f, DConstFieldSpXVx f0) const</argsstring>
        <name>get_derivative</name>
        <qualifiedname>KrookSourceAdaptive::get_derivative</qualifiedname>
        <param>
